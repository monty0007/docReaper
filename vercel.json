{
    "version": 2,
    "builds": [
        {
            "src": "api/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/convert",
            "dest": "api/index.js"
        },
        {
            "src": "/build-pdf",
            "dest": "api/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ],
    "functions": {
        "api/index.js": {
            "maxDuration": 60
        }
    }
}